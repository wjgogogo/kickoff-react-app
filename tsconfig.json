{
  "compilerOptions": {
    "strict": true,
    "module": "ESNext",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react"
  },
  "include": [
    "src",
    "tests"
  ]
}