{
	"name": "kickoff-react-app",
	"version": "0.0.1",
	"description": "a cli tool for generator react boilerplate",
	"main": "index.js",
	"scripts": {
		"start": "ts-node src/index.ts",
		"build": "tsc"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"*.ts": [
			"eslint --fix",
			"prettier --write",
			"git add"
		],
		"*.json": [
			"prettier --write",
			"git add"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wjgogogo/kickoff-react-app.git"
	},
	"keywords": [
		"node",
		"cli tool",
		"react boilerplate"
	],
	"author": "bacteria369 <906626481@qq.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/wjgogogo/kickoff-react-app/issues"
	},
	"homepage": "https://github.com/wjgogogo/kickoff-react-app#readme",
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@types/node": "^14.14.22",
		"@typescript-eslint/eslint-plugin": "^4.14.0",
		"@typescript-eslint/parser": "^4.14.0",
		"eslint": "^7.18.0",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-config-prettier": "^7.2.0",
		"eslint-plugin-import": "^2.22.1",
		"husky": "^4.3.8",
		"lint-staged": "^10.5.3",
		"prettier": "^2.2.1",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.3"
	}
}
