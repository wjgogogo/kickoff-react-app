{
	"name": "kickoff-react-app",
	"version": "0.0.1",
	"description": "a cli tool for generator react boilerplate",
	"main": "index.js",
	"scripts": {
		"start": "ts-node src/index.ts",
		"build": "tsc"
	},
	"bin": {
		"kickoff-react-app": "./bin/kra",
		"kra": "./bin/kra"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"*.ts": [
			"eslint --fix",
			"prettier --write",
			"git add"
		],
		"*.json": [
			"prettier --write",
			"git add"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wjgogogo/kickoff-react-app.git"
	},
	"engines": {
		"node": ">=10"
	},
	"keywords": [
		"node",
		"cli tool",
		"react boilerplate"
	],
	"author": "bacteria369 <906626481@qq.com>",
	"license": "MIT",
	"homepage": "https://github.com/wjgogogo/kickoff-react-app#readme",
	"bugs": {
		"url": "https://github.com/wjgogogo/kickoff-react-app/issues"
	},
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@types/inquirer": "^7.3.1",
		"@types/node": "^14.14.22",
		"@types/semver": "^7.3.4",
		"@types/validate-npm-package-name": "^3.0.2",
		"@typescript-eslint/eslint-plugin": "^4.14.0",
		"@typescript-eslint/parser": "^4.14.0",
		"eslint": "^7.18.0",
		"eslint-config-prettier": "^7.2.0",
		"husky": "^4.3.8",
		"lint-staged": "^10.5.3",
		"prettier": "^2.2.1",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.3"
	},
	"dependencies": {
		"chalk": "^4.1.0",
		"commander": "^6.2.1",
		"download-git-repo": "^3.0.2",
		"inquirer": "^7.3.3",
		"ora": "^5.3.0",
		"semver": "^7.3.4",
		"validate-npm-package-name": "^3.0.0"
	}
}
